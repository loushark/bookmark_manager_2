
<h1>Bookmarks:</h1>

<ul>
<% @bookmarks.each do |bookmark| %>
<li class="bookmark" id="bookmark-<%= bookmark.id %>">
  <a href= <%=bookmark.url%> >
    <%=bookmark.title%>
  </a>
<form action="/bookmarks/<%=bookmark.id%>" method="post">
  <input type='hidden' name='_method' value='DELETE'/>
  <input type="submit" name='delete' value='delete'>
</form>
</li>
<% end %>

</ul>
